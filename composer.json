{
    "name": "ignasimg/big-data-utils",
    "description": "A few prototypes of various utilities to deal with big datasets",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "IgnasiMG\\BigDataUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IgnasiMG\\Tests\\BigDataUtils\\": "tests/"
        }
    },
    "require": {
        "php": ">= 7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "scripts": {
        "test": [
            "@composer install",
            "php vendor/bin/phpunit"
        ]
    }
}
